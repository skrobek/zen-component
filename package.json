{
  "name": "zen-component",
  "displayName": "zen-component",
  "description": "Generate files for zen react component",
  "version": "0.2.0",
  "publisher": "skrobek",
  "engines": {
    "vscode": "^1.28.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skrobek/zen-component.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.zenComponent"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.zenComponent",
        "title": "Create Zencargo Component"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "vscode:package": "vsce package",
    "vscode:publish": "vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^7.10.1",
    "jest": "^23.0.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "vscode": "^1.1.18"
  }
}
